#!/bin/sh
#|-*- mode:lisp -*-|#
#|
exec ros -Q -- $0 "$@"
|#

(ql:quickload :fiveam)

(defun main (&rest argv)
  (declare (ignorable argv))
  (uiop:quit (if (handler-case
                     (progn
                       (asdf:load-system :pddl2.test)
                       (eval (read-from-string "(every #'fiveam::TEST-PASSED-P (5am:run! :pddl2))"))
)
                   (serious-condition (c)
                     (describe c)
                     (uiop:quit 2)))
                 0 1)))
